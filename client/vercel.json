{
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/challenge/:path*", "destination": "/" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "env": {
    "VITE_OPENAI_API_KEY": "${VITE_OPENAI_API_KEY}"
  }
}
